{
  "name": "smarter-home",
  "version": "1.2.0",
  "description": "SmarterHome is a prototype smart home control app for experimenting with features to improve the UX of multi-user smart homes.",
  "main": "smartapp.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "build:web": "webpack --config webpack.config.js",
    "build:cordova-dev": "webpack --config webpack.cordova-dev.js",
    "build:cordova-prod": "webpack --config webpack.cordova-prod.js",
    "clean": "rm -rf dist/ && rm -rf cordova/www/css && rm -rf cordova/www/js",
    "delete": "node scripts/delete-installed.js",
    "docker:build-dev": "docker build -t dev --build-arg server_mode=dev --build-arg node_env=development .",
    "docker:build-prod": "docker build -t prod --build-arg server_mode=prod --build-arg node_env=production .",
    "docker:run-dev": "docker run --net my-mongo-cluster -p 5000:5000 dev",
    "docker:run-prod": "docker run --net my-mongo-cluster -p 5001:5000 prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "webpack --watch --config webpack.config.js",
    "watch:web": "webpack --watch --config webpack.config.js",
    "watch:cordova-dev": "webpack --watch --config webpack.cordova-dev.js",
    "watch:cordova-prod": "webpack --watch --config webpack.cordova-prod.js"
  },
  "author": "Eric Zeng",
  "license": "MIT",
  "dependencies": {
    "@material/checkbox": "^3.2.0",
    "@material/form-field": "^3.2.0",
    "@material/radio": "^3.2.0",
    "@material/react-button": "^0.15.0",
    "@material/react-material-icon": "^0.15.0",
    "@material/react-ripple": "^0.15.0",
    "@material/react-top-app-bar": "^0.15.0",
    "@material/ripple": "^3.2.0",
    "@material/theme": "^3.1.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "colors": "^1.4.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.0.0",
    "cookie": "^0.4.0",
    "cookie-signature": "^1.1.0",
    "core-js": "^3.2.1",
    "csv-parse": "^4.6.3",
    "elliptic": "^6.5.3",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "firebase": "^7.2.0",
    "googleapis": "^43.0.0",
    "history": "^4.10.1",
    "http-signature": "^1.2.0",
    "mongoose": "^5.7.8",
    "mongoose-unique-validator": "^2.0.3",
    "prop-types": "^15.7.2",
    "qrcode": "^1.4.2",
    "react": "^16.10.2",
    "react-beautiful-dnd": "^11.0.5",
    "react-dom": "^16.10.2",
    "react-notify-toast": "^0.5.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.4",
    "request": "^2.88.0",
    "winston": "^3.2.1",
    "winston-mongodb": "^5.0.0",
    "xhr": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-plugin-react": "^7.16.0",
    "glob": "^7.1.4",
